// C++ program to demonstrate
// the problem with 'char'
#include <iostream>

using namespace std;

int main()
{
	for (char a = 0; a <= 225; a++)
		cout << a;
	return 0;
}

// C++ program to demonstrate
// the problem with 'bool'
#include <iostream>

using namespace std;

int main()
{
	// declaring Boolean
	// variable with true value
	bool a = true;

	for (a = 1; a <= 5; a++)
		cout << a;

	return 0;
}

// C++ program to demonstrate
// the problem with 'short'
#include <iostream>

using namespace std;

int main()
{
	// declaring short variable
	short a;

	for (a = 32767; a < 32770; a++)
		cout << a << "\n";

	return 0;
}

// C++ program to demonstrate
// the problem with 'unsigned short'
#include <iostream>

using namespace std;

int main()
{
	unsigned short a;

	for (a = 65532; a < 65536; a++)
		cout << a << "\n";

	return 0;
}

